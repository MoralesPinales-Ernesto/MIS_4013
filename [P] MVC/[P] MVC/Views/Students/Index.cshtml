@model List<string>
@{
    ViewData["Title"] = "Students";
}
<img src="https://images.unsplash.com/photo-1571260899304-425eee4c7efc?ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&q=80&w=1170"
     class="img-fluid rounded mb-4" alt="Students" style="max-height: 1000px; object-fit: cover; width: 100%;" />
<h2 class="mb-4">Student List</h2>
<div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
    @foreach (var student in Model)
    {
        <div class="col">
            <div class="card text-center h-100" style="background-color: #f8f9fa;">
                <div class="card-body d-flex align-items-center justify-content-center">
                    <h5 class="card-title mb-0">@student</h5>
                </div>
            </div>
        </div>
    }
</div>